class Edge():

    def __init__(self,nodoIni = None,nodoFin = None,peso = 0):
        self.ini = nodoIni
        self.fin = nodoFin
        self.peso = peso

    def agregar_fin(vertice):
        self.fin = vertice

    def agrega_inicio(vertice):
        self.ini = vertice

    def tvertex():
        return self.fin

    def svertex():
        return self.ini

    def weight():
        return self.peso
    
    def set_weight(w):
        self.peso = w
